public class Animal {
  // atributos

  private String nome;
  private double peso;

  // Metodo Construtor
  public Animal(String nome, double peso) {
    this.nome = nome;
    this.peso = 0;

  }

  public Animal(InputStream in) {
}

public String getNome() {
    return nome;
  }

  public void setNome(String nome) {
    this.nome = nome;
  }

  public double getPeso() {
    return peso;
  }

  public void setPeso(double peso) {
    this.peso = peso;
  }
}
