public class Cachorro extends Animal{
   private String raça;
   // metodo construtor
   public Cachorro(String nome,double peso, String raça){
      super(nome,peso);
      this.raça = raça;
   }
public String getRaça() {
	return raça;
}
public void setRaça(String raça) {
	this.raça = raça;
}
  

}
